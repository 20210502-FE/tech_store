                                    a.product__reviews-link(href="#") Reviews (#{product.rewiewsCount})
                                .product__title=product.title.toUpperCase()
                                if product.newPrice
                                    .product__price.product__price_linethrough #{product.currency}!{product.primaryPrice}
                                    .product__price-new #{product.currency}#{product.newPrice}
                                else 
                                    .product__price #{product.currency}!{product.primaryPrice}

        .product__column
            article.product__article
                .product__catalog.product__catalog-monitor
                    .product__catalog-title Gaming Monitors
                    a.product__catalog-link(href="#") See All Products
            .product__wrapper
                each product in productMonitors 
                    article.product__article.product__article-item
                        .product__button-wrapper
                            button.product__button Add To Cart
                        button.product__rating.product__rating-btn
                            img.product__rating-img(src="assets/icon/rating.svg", alt="rating")
                        button.product__like.product__like-btn
                            img.product__like-img(src="assets/icon/like.svg", alt="like")
                        a.product__link(href="#")
                            .product__article-inner
                                if product.inStock
                                    .product__stock.product__stock-in in Stock 
                                else 
                                    .product__stock.product__stock-check check availability
                                .product__img-wrapper
                                    img.product__img(src=product.image, alt = product.imageAlt)
